<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.google.android.material.chip.Chip
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/history"
        style="@style/Widget.Material3.Chip.Input"
        android:textSize="14sp"
        android:textColor="@color/text_dark"
        android:maxLines="1"
        android:ellipsize="end"
        android:checkable="false"
        app:chipBackgroundColor="@color/gray"
        app:chipStrokeColor="@color/gray_filled_btn_ripple"
        app:rippleColor="@color/gray_filled_btn_ripple"
        tools:text="test test test test test test test test test test test test test test "/>

</FrameLayout>